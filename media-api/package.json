    "@nestjs/cli": "^9.4.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/microservices": "^9.4.0",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/terminus": "^9.2.2",
    "@opentelemetry/api": "^1.4.0",
    "@opentelemetry/auto-instrumentations-node": "^0.36.3",
