    "@opentelemetry/sdk-trace-base": "^1.9.1",
    "@opentelemetry/semantic-conventions": "^1.9.1",
    "@prisma/instrumentation": "^4.14.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "joi": "^17.9.2",
    "mongoose": "^7.1.1",
    "nest-winston": "^1.9.2",
    "nestjs-grpc-reflection": "^0.0.21",
